generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Link {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  url       String   @unique
  shortUrl  String   @unique
  createdAt DateTime @default(now()) // Campo para la fecha de creaci√≥n
}
